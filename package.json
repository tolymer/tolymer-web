{
  "name": "tolymer-web",
  "version": "0.0.0",
  "description": "Tolymer Web",
  "author": "1000ch <shogo.sensui@gmail.com>",
  "private": true,
  "engines": {
    "node": "^10.0.0",
    "npm": "^6.0.0"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "7700"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "fix": "prettier '**/*.{vue,js}' --write",
    "fix:css": "stylelint --fix '**/*.vue' '**/*.css'",
    "lint": "npm run lint:js && prettier '**/*.{vue,js}' --list-different && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint '**/*.vue' '**/*.css'",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "storybook": "start-storybook -p 6006 -s src/assets",
    "build-storybook": "build-storybook -s src/assets"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.1.1",
    "body-parser": "^1.18.2",
    "chart.js": "^2.7.2",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.29.0",
    "normalize.css": "^8.0.0",
    "nuxt": "^2.3.4",
    "vue-chartjs": "^3.3.1"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "prettier": "^1.11.1",
    "stylelint": "^9.4.0",
    "stylelint-config-fresh": "^2.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.2.2"
  }
}
