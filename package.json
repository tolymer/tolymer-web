{
  "name": "tolymer-web",
  "version": "0.0.0",
  "description": "Tolymer Web",
  "author": "1000ch <shogo.sensui@gmail.com>",
  "private": true,
  "engines": {
    "node": "^12.0.0",
    "npm": "^6.0.0"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "7700"
    }
  },
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "fix": "npm run fix:js && npm run fix:css",
    "fix:js": "npm run lint:js --fix && prettier '**/*.{vue,js}' --write",
    "fix:css": "npm run lint:css -- --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint '**/*.vue' '**/*.css'",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.9.0",
    "body-parser": "^1.18.2",
    "chart.js": "^2.7.2",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.3",
    "cookie-universal-nuxt": "^2.1.3",
    "date-fns": "^2.9.0",
    "normalize.css": "^8.0.0",
    "nuxt": "^2.14.0",
    "vue-chartjs": "^3.3.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.4",
    "@nuxt/typescript-build": "^2.0.3",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "^2.0.0",
    "stylelint": "^13.0.0",
    "stylelint-config-mercari": "^0.10.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vuex-smart-module": "^0.4.5"
  }
}
