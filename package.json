{
  "name": "tolymer-web",
  "version": "1.0.0",
  "description": "Tolymer Web",
  "author": "1000ch <shogo.sensui@gmail.com>",
  "private": true,
  "engines": {
    "node": "8.11.1",
    "npm": "5.8.0"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "5000"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
